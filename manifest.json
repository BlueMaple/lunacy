{
  "name": "Lunacy 2",
  "description": "Survive in a village menaced by werewolves!",
  "manifest_version": 2,
  "version": "1.9.6",
  "minimum_chrome_version": "25",
  "app": {
    "launch": {
      "local_path": "html/index.nocache.deploy.html",
      "container": "panel",
      "width": 320,
      "height": 548
    }
  },
  "permissions": [
    "notifications",
    "storage",
    "http://*.playlunacy.com/"
  ],
  "web_accessible_resources": [
    "visuals/icons/lunacy48.png"
  ],
  "icons": {
    "16": "visuals/icons/lunacy16.png",
    "48": "visuals/icons/lunacy48.png",
    "128": "visuals/icons/lunacy128.png"
  }
}
